#include<iostream>
using namespace std;

int main() {

	for (int i = 0; i < 5; i++) { 
		for (int j = 0; j < 5; j++) { 
			cout << "B";
			
		}
		cout << endl; 


	}
	cout << "           " << endl;

	for (int i = 0; i < 8; i++) {
		for (int j = 0; j < 3; j++) {
			cout << "R";

		}
		cout << endl;


	}

	cout << "           " << endl;

	for (int i = 0; i < 4; i++) {
		for (int j = 0; j < 9; j++) {
			cout << "U";

		}
		cout << endl;


	}

	cout << "           " << endl;

	for (int i = 0; i < 4; i++) {
		for (int j = 0; j < 20; j++) {
			cout << "H";

		}
		cout << endl;


	}
}
